{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "C/C++: g++ debug specified program",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/build/minidbg",
        "args": [ "hello4" ],
        "stopAtEntry": true,
        "cwd": "${workspaceFolder}/build",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "miDebuggerPath": "/usr/bin/gdb",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ],
        "preLaunchTask": ""
      }
    ]
  }